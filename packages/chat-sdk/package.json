{
  "name": "@coze/chat-sdk",
  "version": "0.1.8",
  "description": "Coze chat components for taro",
  "author": "gaoding.devingao@bytedance.com",
  "exports": {
    ".": "./dist/lib-source/src/exports/index.js",
    "./webJs": {
      "import": "./dist/lib-h5/exports/index.js",
      "types": "./dist/lib-source/src/exports/index.d.ts"
    },
    "./webCss": "./dist/lib-h5/exports/index.css",
    "./types": "./dist/lib-source/src/exports/index.d.ts"
  },
  "main": "./dist/lib-source/src/exports/index.js",
  "types": "./dist/lib-source/src/exports/index.d.ts",
  "typesVersions": {
    "*": {
      "webJs": [
        "./dist/lib-source/src/exports/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.map",
    "README.md"
  ],
  "scripts": {
    "build:alipay": "taro build --type alipay",
    "build:h5": "taro build --type h5",
    "build:h5:lib": "rm -rf ./dist/lib-h5/ && cross-env BUILD_NATIVE=true taro build native-components --type h5 --no-inject-global-style",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "build:jd": "taro build --type jd",
    "build:lib": "npm run build:taro:source && npm run build:h5:lib && npm run build:weapp:lib",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:rn": "taro build --type rn",
    "build:swan": "taro build --type swan",
    "build:taro:source": "rm -rf ./lib-ts && rm -rf ./dist/lib-source/ && ttsc -b tsconfig.build.json && ./scripts/lib-source.sh",
    "build:tt": "taro build --type tt",
    "build:weapp": "taro build --type weapp",
    "build:weapp:lib": "rm -rf ./dist/lib-weapp/ && cross-env BUILD_NATIVE=true taro build native-components --type weapp",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "npm run build:lib",
    "publish:lib:byte": "./scripts/lib-publish.sh",
    "publish:lib:npm": "npm publish",
    "test": "vitest --run",
    "test:cov": "vitest --coverage --run"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@coze/api": "^1.0.17",
    "classnames": "^2.3.2",
    "extendable-media-recorder": "^9.2.19",
    "mdast": "^3.0.0-alpha.6",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm-strikethrough": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-radio-list-item": "^2.1.0",
    "micromark-extension-gfm-strikethrough": "2.1.0",
    "micromark-extension-gfm-table": "^2.1.0",
    "micromark-extension-gfm-task-list-item": "^2.1.0",
    "micromark-extension-misc-radio-list-item": "^2.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-transform-unicode-property-regex": "^7.25.9",
    "@babel/preset-react": "~7.13.13",
    "@babel/runtime": "^7.24.4",
    "@coze-infra/eslint-config": "workspace:*",
    "@coze-infra/ts-config": "workspace:*",
    "@coze-infra/vitest-config": "workspace:*",
    "@rsbuild/plugin-less": "^1.1.0",
    "@rsbuild/plugin-react": "^1.1.0",
    "@swc/types": "^0.1.17",
    "@tarojs/cli": "4.0.7",
    "@tarojs/components": "4.0.7",
    "@tarojs/helper": "4.0.7",
    "@tarojs/plugin-framework-react": "4.0.7",
    "@tarojs/plugin-http": "^4.0.7",
    "@tarojs/plugin-platform-alipay": "4.0.7",
    "@tarojs/plugin-platform-h5": "4.0.7",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.7",
    "@tarojs/plugin-platform-jd": "4.0.7",
    "@tarojs/plugin-platform-qq": "4.0.7",
    "@tarojs/plugin-platform-swan": "4.0.7",
    "@tarojs/plugin-platform-tt": "4.0.7",
    "@tarojs/plugin-platform-weapp": "4.0.7",
    "@tarojs/react": "4.0.7",
    "@tarojs/runtime": "4.0.7",
    "@tarojs/service": "4.0.7",
    "@tarojs/shared": "4.0.7",
    "@tarojs/taro": "4.0.7",
    "@tarojs/taro-loader": "4.0.7",
    "@tarojs/webpack5-runner": "4.0.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/mdast": "4.0.3",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@types/showdown": "^2.0.6",
    "@types/webpack-env": "^1.13.6",
    "@vitest/coverage-v8": "~2.1.4",
    "axios": "^1.7.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-taro": "4.0.7",
    "core-js": "^3.39.0",
    "cross-env": "^7.0.3",
    "dts": "^0.1.1",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.11.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "ttsc": "^0.3.1",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.5.2",
    "vitest": "~2.1.4",
    "webpack": "^5.64.4"
  },
  "peerDependencies": {
    "axios": "^1.7.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "miniprogram": "dist/lib-weapp",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Less",
    "framework": "React"
  }
}
