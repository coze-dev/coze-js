{
  "name": "@coze-infra/npm-mcp-server",
  "version": "0.0.1-alpha.1",
  "description": "NPM MCP Server",
  "repository": {
    "type": "git",
    "url": "https://github.com/coze-dev/rush-arch",
    "directory": "mcp/npm"
  },
  "license": "MIT",
  "author": "tecvan.fe@gmail.com",
  "bin": {
    "npm-mcp": "bin/main"
  },
  "files": [
    "bin",
    "dist",
    "CHANGELOG.md",
    "!**/*.map",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "lint": "eslint ./ --cache",
    "serve": "tsx src/cli.ts serve --port 3000",
    "start": "tsx src/cli.ts start",
    "test": "vitest --run __tests__",
    "test:cov": "npm run test -- --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@rushstack/node-core-library": "^5.12.0",
    "axios": "^1.8.4",
    "commander": "^13.1.0",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-meta": "^0.0.1",
    "morgan": "^1.10.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@coze-infra/eslint-config": "workspace:*",
    "@coze-infra/ts-config": "workspace:*",
    "@coze-infra/vitest-config": "workspace:*",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "~6.0.0",
    "@rollup/plugin-node-resolve": "~15.0.1",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.13",
    "@vitest/coverage-v8": "^3.0.9",
    "rollup": "^4.9.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
